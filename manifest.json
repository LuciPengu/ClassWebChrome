{
  "manifest_version": 3,
  "name": "Class-Web Fix!",
  "version": "1.1",
  "description": "Inject new code into Las Positas Class-Web",
  "permissions": ["activeTab"],
  "icons": {
    "48": "images/icon48.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "images/icon48.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://bw11.clpccd.cc.ca.us/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}
